package com.connectik.cloverlead.dto;

import com.connectik.cloverlead.model.Opportunity;

import java.util.ArrayList;
import java.util.List;

public class OpportunityOutput implements EntityOutput {

    private Long id;

    private SimpleUserOutput createdBy;

    private SimpleUserOutput updatedBy;

    private String createdAt;

    private String updatedAt;

    private String title;
    
    private Opportunity.Category category;

    private Opportunity.Type type;

    private String description;

    private List<String> keywords = new ArrayList<>();

    private List<String> specialties = new ArrayList<>();

    private String startDate;

    private String endDate;

    private String estimatedValueCurrency;

    private Integer estimatedValue;

    private AddressDTO address;

    private SimpleCompanyOutput company;

    @Override
    public Long getId() {
        return id;
    }

    @Override
    public void setId(Long id) {
        this.id = id;
    }

    public SimpleCompanyOutput getCompany() {
        return company;
    }

    public void setCompany(SimpleCompanyOutput company) {
        this.company = company;
    }

    public SimpleUserOutput getCreatedBy() {
        return createdBy;
    }

    public void setCreatedBy(SimpleUserOutput createdBy) {
        this.createdBy = createdBy;
    }

    public SimpleUserOutput getUpdatedBy() {
        return updatedBy;
    }

    public void setUpdatedBy(SimpleUserOutput updatedBy) {
        this.updatedBy = updatedBy;
    }

    public String getCreatedAt() {
        return createdAt;
    }

    public void setCreatedAt(String createdAt) {
        this.createdAt = createdAt;
    }

    public String getUpdatedAt() {
        return updatedAt;
    }

    public void setUpdatedAt(String updatedAt) {
        this.updatedAt = updatedAt;
    }

    public String getTitle() {
        return title;
    }

    public void setTitle(String title) {
        this.title = title;
    }

    public Opportunity.Category getCategory() {
        return category;
    }

    public void setCategory(Opportunity.Category category) {
        this.category = category;
    }

    public Opportunity.Type getType() {
        return type;
    }

    public void setType(Opportunity.Type type) {
        this.type = type;
    }

    public String getDescription() {
        return description;
    }

    public void setDescription(String description) {
        this.description = description;
    }

    public List<String> getKeywords() {
        return keywords;
    }

    public void setKeywords(List<String> keywords) {
        this.keywords = keywords;
    }

    public List<String> getSpecialties() {
        return specialties;
    }

    public void setSpecialties(List<String> specialties) {
        this.specialties = specialties;
    }

    public String getStartDate() {
        return startDate;
    }

    public void setStartDate(String startDate) {
        this.startDate = startDate;
    }

    public String getEndDate() {
        return endDate;
    }

    public void setEndDate(String endDate) {
        this.endDate = endDate;
    }

    public String getEstimatedValueCurrency() {
        return estimatedValueCurrency;
    }

    public void setEstimatedValueCurrency(String estimatedValueCurrency) {
        this.estimatedValueCurrency = estimatedValueCurrency;
    }

    public Integer getEstimatedValue() {
        return estimatedValue;
    }

    public void setEstimatedValue(Integer estimatedValue) {
        this.estimatedValue = estimatedValue;
    }

    public AddressDTO getAddress() {
        return address;
    }

    public void setAddress(AddressDTO address) {
        this.address = address;
    }
}
